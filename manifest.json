{
  "manifest_version": 2,
  "name": "dependabot rebase button",
  "description": "add [@dependabot rebase] to PR comment",
  "version": "1.0",
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ]
}
